<FindBugsFilter>
	<!-- Exclude generated code in queryParsers packages -->
    <Match>
        <Package name="~nl\.inl\.blacklab\.queryParser.*" />
    </Match>
    <Match>
        <Bug pattern="IL_INFINITE_RECURSIVE_LOOP " />
        <Bug pattern="NP_UNWRITTEN_FIELD" />
    </Match>
</FindBugsFilter>